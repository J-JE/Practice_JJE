#query.properties
selectAll=SELECT * FROM PRODUCT_STOCK
insertProduct=INSERT INTO PRODUCT_STOCK VALUES(?, ?, ?, ?, DEFAULT)
updateProduct=UPDATE PRODUCT_STOCK SET P_NAME = ?, PRICE = ?, DESCRIPTION = ? WHERE PRODUCT_ID = ?
deleteProduct=DELETE FROM PRODUCT_STOCK WHERE PRODUCT_ID = ?
searchProduct=SELECT * FROM PRODUCT_STOCK WHERE P_NAME LIKE ?
selectAllIo=SELECT * FROM PRODUCT_IO JOIN PRODUCT_STOCK USING(PRODUCT_ID) ORDER BY IO_NUM
selectInOut=SELECT * FROM PRODUCT_IO JOIN PRODUCT_STOCK USING(PRODUCT_ID) WHERE STATUS = ? ORDER BY IO_NUM
ProductInOut=INSERT INTO PRODUCT_IO VALUES(SEQ_PRODUCT_IO.NEXTVAL, ?, SYSDATE, ?, ?)