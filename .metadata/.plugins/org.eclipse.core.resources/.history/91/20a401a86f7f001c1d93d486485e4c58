package com.kh2.collection.silsub1.model.dao;

import java.util.ArrayList;

import com.kh2.collection.silsub1.model.vo.Board;

public class BoardDao {
	private ArrayList<Board> list;
	
	public BoardDao() {	}
	
	public void getLastBoardNo() : int
	public void writeBoard(board:Board) : void
	public void displayAllList() : ArrayList<Board>
	public void displayBoard() : Board
	public void upReadCount(no:int) : void 
	public void modifyTitle(no:int, title:String) : void
	public void modifyContent(no:int, content:String) : void
	public void deleteBoard(no:int) : void 
	public void searchBoard(title:String) : ArrayList<Board>
	public void saveListFile() : void

	
	
}
